		"cta": {
			"type": "object",
			"default": {
				"url": "",
				"text": "Learn More",
				"show": false
			}
		}
	},
	"supports": {
		"html": false,
		"align": true
	},
	"textdomain": "bts",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./primary-hero.php",
	"viewScript": "file:./view.js"
}
