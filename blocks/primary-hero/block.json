{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "bts/primary-hero",
	"version": "0.1.0",
	"title": "Primary Hero",
	"category": "text",
	"icon": "align-full-width",
	"attributes": {
		"title": {
			"type": "object",
			"default": {
				"text": "",
				"textColor": "",
